    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "electron-is-dev": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "3.1.2",
    "styled-components": "^4.4.0",
    "typescript": "3.6.3"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "concurrently": "^4.1.2",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "wait-on": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
