{
  "name": "prodo",
  "description": "Snippets manager",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "homepage": "./",
  "main": "public/electron.js",
  "author": {
    "name": "Ali Zahid",
    "email": "ali.zahid@live.com",
    "url": "https://alizahid.dev"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:render": "sed -i '/homepage/d' package.json ; yarn build",
    "dev": "concurrently 'BROWSER=none yarn start' 'wait-on http://localhost:3000 && electron .'",
    "dist": "yarn build; electron-builder",
    "start": "react-scripts start"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "easy-peasy": "^3.1.0",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.0.0",
    "electron-window-state": "^5.0.3",
    "firebase": "^7.1.0",
    "moment": "^2.24.0",
    "nanoid": "^2.1.2",
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "secure-ls": "^1.2.5",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "24.0.18",
    "@types/nanoid": "^2.1.0",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "concurrently": "^5.0.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "husky": "^3.0.8",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.6.4",
    "wait-on": "^3.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "build": {
    "appId": "com.designplox.prodo",
    "productName": "Prodo",
    "copyright": "Copyright Â© 2020 Ali Zahid",
    "directories": {
      "buildResources": "public"
    }
  }
}
